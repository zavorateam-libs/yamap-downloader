{
  "name": "YaMap_3D-Viewer",
  "version": "0.9.7",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.yamap.3dpro",
    "productName": "YaMap 3D Pro",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "assets/icon.svg"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Graphics",
      "icon": "assets/icon.svg"
    },
    "files": [
      "**/*",
      "!dist/*"
    ]
  }
}
